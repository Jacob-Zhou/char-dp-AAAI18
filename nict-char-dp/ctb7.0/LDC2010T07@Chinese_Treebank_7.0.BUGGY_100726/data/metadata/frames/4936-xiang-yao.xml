<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id> 想要 </id>
    <frameset id="f1" edef="want to do something">
        <role argnum="0" argrole="agent"/>
        <role argnum="1" argrole="predicate"/>
         
        <frame>
            <mapping>
                <V/>
                <mapitem src="sbj" trg="arg0"/>
                <mapitem src="ipobj" trg="arg1"/>
            </mapping>
            <example>
                <parse>
                    (NP-PN-SBJ (NR 英国))
                    (VP (VV 想要)
                    (IP-OBJ (NP-SBJ (-NONE- *PRO*))
                    (VP (VV 保留)
                    (NP-OBJ (NN 北部)
                    (NN 禁飞区)))) 
                    
                </parse>
                <arg n="0">
                    英国	
                </arg>
                <arg n="1">
                    北部 禁飞区	
                </arg>
                <V/>
            </example>
        </frame>
    </frameset>
    <frameset id="f2" edef="want">
        <role argnum="0" argrole="entity described"/>
        <role argnum="1" argrole="entity arg0 wants"/>
        <frame>
            <mapping>
                <V/>
                <mapitem src="sbj" trg="arg0"/>
            </mapping>
            <example>
                <parse>
                    (NP-SBJ (PN 我))
                    (VP (ADVP (AD 只))
                    (VP (VV 想要)
                    (NP-OBJ (ADJP (JJ 最后))
                    (NP (NN 结果)))))) 
                    
                </parse>
                <arg n="0">
                    我	
                </arg>
                <arg n="m" f="adv">
                    只	
                </arg>
                <arg n="1">
                    最后 结果	
                </arg>
                <V/>
            </example>
        </frame>
    </frameset>
</verb>
