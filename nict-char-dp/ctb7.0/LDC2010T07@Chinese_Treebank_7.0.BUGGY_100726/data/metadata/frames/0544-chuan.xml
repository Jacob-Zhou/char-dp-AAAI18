            <mapping>
                <V/>
                <mapitem src="sbj" trg="arg0"/>
                <mapitem src="npobj" trg="arg1"/>
            </mapping>
            <example>
                <parse>
                    (NP-SBJ (CP (WHNP-1 (-NONE- *OP*))
                    (CP (IP (NP-SBJ (PN 他))
                    (VP (PP-DIR (P 给)
                    (NP (NN 球员们)))
                    (VP (VV 传)
                    (NP-OBJ (-NONE- *T*-1)))))
                    (DEC 的)))
                    (NP (NN 方案))) 
                </parse>
                <arg n="0">
                    他 	  
                </arg>
                <arg n="1">
                    *T*-1 	  
                </arg>
                <arg n="2">
                    给 球员们 	  
                </arg>
            </example>
        </frame>
    </frameset>
</verb>
