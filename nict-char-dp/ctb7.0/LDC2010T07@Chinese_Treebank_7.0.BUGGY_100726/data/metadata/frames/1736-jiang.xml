            </mapping>
            <example>
                <parse>
                    (IP-IMP (NP-SBJ (-NONE- *pro*))
                    (VP (VV 降)
                    (NP-OBJ (NP (NP-PN (NR 英国))
                    (NP (NN 国旗)))
                    (CC 及)
                    (NP (NP-PN (NR 香港))
                    (NP (NN 旗)))))) 
                    
                </parse>
                <arg n="0">
                    *pro* 	  	  
                </arg>
                <arg n="1">
                    英国 国旗 及 香港 旗 	  	  
                </arg>
            </example>
        </frame>
    </frameset>
